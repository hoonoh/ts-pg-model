{
  "compilerOptions": {
    "target": "ES2019",
    "module": "nodenext",
    "lib": ["DOM", "ES5", "ESNext"],
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
