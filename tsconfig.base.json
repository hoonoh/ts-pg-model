{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "lib": ["DOM", "ES5", "ESNext"],
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
